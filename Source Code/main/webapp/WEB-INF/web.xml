<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>currencyConvert</servlet-name>
  	<servlet-class>
  		org.springframework.web.servlet.DispatcherServlet
  	</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>dataSource</servlet-name>
  	<display-name>dataSource</display-name>
  	<description></description>
  	<servlet-class>currencyConvert.control.dataSource</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DataSource</servlet-name>
  	<display-name>DataSource</display-name>
  	<description></description>
  	<servlet-class>currencyConvert.control.DataSource</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Fetch</servlet-name>
  	<display-name>Fetch</display-name>
  	<description></description>
  	<servlet-class>currencyConvert.dataAccess.Fetch</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Fetch</servlet-name>
  	<url-pattern>/Fetch</url-pattern>
  </servlet-mapping>
</web-app>
